// ============================================
// DESIGN WORKFLOW MODELS (Intelligent Specification & Design Assistant)
// ============================================

// Design Projects table - AI-powered property redesign/construction projects
model DesignProject {
  id                String              @id @default(cuid())
  userId            String
  name              String
  propertyType      PropertyType        @default(RESIDENTIAL)
  location          String              // For compliance and climate considerations
  budgetRange       BudgetRange?
  status            DesignProjectStatus @default(DRAFT)

  // Project Metadata
  description       String?
  notes             String?
  totalEstimate     Float?              // Total cost estimate
  currency          String              @default("CRC")

  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt
  completedAt       DateTime?

  // Relationships
  user              User                @relation(fields: [userId], references: [id])
  areas             DesignArea[]

  @@index([userId])
  @@index([status])
  @@index([createdAt])
}

// Design Areas table - Individual areas/rooms within a design project
model DesignArea {
  id                  String                @id @default(cuid())
  projectId           String
  areaType            AreaType              // Kitchen, bathroom, etc.
  areaName            String                // Custom name: "Main Kitchen", "Master Bathroom"
  status              AreaStatus            @default(NOT_STARTED)

  // Requirements & Context
  userRequirements    String?               // Voice/text input from user
  voiceTranscript     String?               // Raw transcript from speech-to-text
  visionAnalysis      String?               // JSON with AI vision analysis of uploaded images

  // Cost Estimation
  estimatedCost       Float?
  materialsCost       Float?
  laborCost           Float?
  confidenceLevel     Float?                // AI confidence in estimate (0-1)

  // Metadata
  notes               String?
  squareMeters        Float?

  createdAt           DateTime              @default(now())
  updatedAt           DateTime              @updatedAt

  // Relationships
  project             DesignProject         @relation(fields: [projectId], references: [id], onDelete: Cascade)
  images              AreaImage[]
  specifications      AreaSpecification[]

  @@index([projectId])
  @@index([status])
  @@index([areaType])
}

// Area Images table - Photos uploaded for each area
model AreaImage {
  id                String              @id @default(cuid())
  areaId            String

  // Image Storage
  imageUrl          String              // Stored in public/uploads or cloud storage
  imageName         String
  thumbnailUrl      String?

  // Image Metadata
  fileSize          Int?                // in bytes
  mimeType          String?
  width             Int?
  height            Int?

  // AI Analysis
  visionAnalysis    String?             // JSON with detected materials, colors, style, dimensions
  detectedItems     String?             // JSON array of detected items (faucet, countertop, etc.)
  aiTags            String?             // Comma-separated tags for search

  uploadedAt        DateTime            @default(now())

  // Relationships
  area              DesignArea          @relation(fields: [areaId], references: [id], onDelete: Cascade)

  @@index([areaId])
}

// Area Specifications table - Products/materials selected for each area
model AreaSpecification {
  id                String              @id @default(cuid())
  areaId            String

  // Product Information
  productSku        String
  productName       String
  brand             String              // KOHLER, Schlage, etc.
  category          String              // Grifería, Pisos, etc.
  subcategory       String?

  // Quantity & Pricing
  quantity          Float               // Can be decimal for materials (sq ft, linear ft)
  unit              String              @default("unit") // unit, sq_ft, linear_ft, sq_m
  unitPrice         Float
  totalPrice        Float

  // Additional Details
  finish            String?             // Chrome, Matte Black, etc.
  color             String?
  specifications    String?             // JSON with detailed specs
  notes             String?

  // AI Recommendation
  recommendedByAI   Boolean             @default(false)
  aiConfidence      Float?              // 0-1 confidence score
  alternatives      String?             // JSON array of alternative product SKUs

  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt

  // Relationships
  area              DesignArea          @relation(fields: [areaId], references: [id], onDelete: Cascade)

  @@index([areaId])
  @@index([productSku])
  @@index([category])
}

// ============================================
