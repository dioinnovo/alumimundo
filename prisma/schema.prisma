  SKIPPED
}

// ============================================
// BUDGET ESTIMATION MODELS (Door & Hardware Budget System)
// ============================================

// Budget Estimations - Architectural drawing-based door/hardware budgets
model BudgetEstimation {
  id                String              @id @default(cuid())
  projectNumber     String              @unique
  projectName       String
  clientName        String
  clientEmail       String?
  clientPhone       String?

  // Property Information
  propertyType      PropertyType        @default(RESIDENTIAL)
  location          String
  address           String?
  city              String?
  province          String?

  // Project Details
  status            EstimationStatus    @default(DRAFT)
  totalDoors        Int                 @default(0)
  totalHardware     Int                 @default(0)
  totalCost         Float?
  currency          String              @default("CRC")

  // Blueprint Information
  blueprintScale    String?             // "1:100", "1/4\" = 1'"
  sourceFormat      String?             // "PDF", "DWG", "DXF"
  togalProjectId    String?             // Togal.AI project ID if using their API

  // Timeline
  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt
  completedAt       DateTime?

  // Metadata
  notes             String?

  // Relationships
  items             EstimationItem[]
  documents         EstimationDocument[]

  @@index([status])
  @@index([createdAt])
  @@index([propertyType])
}

// Estimation Items - Individual doors and hardware components
model EstimationItem {
  id                String              @id @default(cuid())
  estimationId      String
  estimation        BudgetEstimation    @relation(fields: [estimationId], references: [id], onDelete: Cascade)

  // Item Classification
  itemType          ItemType            // DOOR, HINGE, LOCK, etc.
  doorType          DoorType?           // For doors only

  // Location Information
  locationId        String
  location          ItemLocation        @relation(fields: [locationId], references: [id])

  // Product Specification
  productSku        String?
  productName       String
  brand             String?             // Schlage, Steelcraft, etc.
  finish            String?
  specifications    String?             // JSON with detailed specs

  // Dimensions (store both metric and imperial for catalog matching)
  dimensionMetric   String?             // "90cm × 210cm"
  dimensionImperial String?             // "36\" × 84\""
  dimensionWidth    Float?              // Normalized width (in cm)
  dimensionHeight   Float?              // Normalized height (in cm)
  dimensionUnit     String?             // "CM" or "INCH" - base unit used

  // Quantity & Pricing
  quantity          Int                 @default(1)
  unitPrice         Float?
  totalPrice        Float?

  // AI Detection Metadata
  detectedByAI      Boolean             @default(false)
  aiConfidence      Float?              // 0-1 confidence score
  detectionSource   String?             // "TOGAL_AI", "CAD_PARSING", "MANUAL"
  catalogMatch      String?             // JSON: matched products with confidence scores
  manuallyReviewed  Boolean             @default(false)

  // Installation
  installationStatus InstallationStatus @default(NOT_STARTED)
  installedAt       DateTime?
  installedBy       String?
  photoUrl          String?             // Verification photo
  notes             String?

  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt

  @@index([estimationId])
  @@index([itemType])
  @@index([doorType])
  @@index([locationId])
  @@index([installationStatus])
}

// Item Locations - Floor, area, and door mark tracking
model ItemLocation {
  id                String              @id @default(cuid())
  floor             String              // "F1", "F2", "Planta Baja", "Ground Floor"
  area              String              // "Cocina", "Habitación 1", "Entrance"
  mark              String              // "P-1-F1", "Door-1-F2", "D-101"
  fullLocation      String              // "F2 - Habitación Principal - P-1-F2"

  // Drawing Coordinates (optional)
  drawingX          Float?
  drawingY          Float?
  drawingPage       Int?                // PDF page number where detected

  // Metadata
  notes             String?

  // Relationships
  items             EstimationItem[]

  @@unique([floor, area, mark])
  @@index([floor])
  @@index([area])
}

// Manufacturer Product Catalog - For catalog matching and pricing
model ManufacturerProduct {
  id                String              @id @default(cuid())

  // Manufacturer Information
  manufacturer      String              // "Schlage", "Steelcraft", "KOHLER", etc.
  sku               String              @unique
  productName       String
  category          String              // "Doors", "Locks", "Hinges", "Faucets", etc.
  subcategory       String?

  // Dimensions (store both metric and imperial for flexible matching)
  widthMetric       Float?              // Width in cm
  heightMetric      Float?              // Height in cm
  depthMetric       Float?              // Depth/thickness in cm
  widthImperial     Float?              // Width in inches
  heightImperial    Float?              // Height in inches
  depthImperial     Float?              // Depth/thickness in inches

  // Product Details
  material          String?             // "Steel", "Aluminum", "Wood", etc.
  finish            String?             // "Satin Chrome", "Oil Rubbed Bronze", etc.
  color             String?

  // Pricing (Costa Rica - CRC)
  unitPrice         Float               // Price in CRC
  currency          String              @default("CRC")
  priceEffectiveDate DateTime?          // When this price became effective

  // Specifications
  specifications    String?             // JSON: detailed technical specs
  installationNotes String?
  isADACompliant    Boolean             @default(false)
  isMarineGrade     Boolean             @default(false)

  // Availability
  leadTimeDays      Int?                // Manufacturing/delivery lead time
  inStock           Boolean             @default(true)
  stockQuantity     Int?

  // Metadata
  catalogPage       String?             // Page number in manufacturer catalog
  catalogYear       Int?
  imageUrl          String?
  pdfUrl            String?             // Product spec sheet URL

  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt

  @@index([manufacturer])
  @@index([category])
  @@index([sku])
  @@index([productName])
}

// Estimation Documents - Uploaded drawings and generated reports
model EstimationDocument {
  id                String              @id @default(cuid())
  estimationId      String
  estimation        BudgetEstimation    @relation(fields: [estimationId], references: [id], onDelete: Cascade)

  // File Information
  filename          String
  originalName      String
  mimeType          String
  size              Int
  url               String

  // Document Type
  documentType      EstimationDocType   @default(ARCHITECTURAL_DRAWING)

  // PDF Parsing Results (for architectural drawings)
  pageCount         Int?
  extractedText     String?             // Text annotations from drawing
  detectedItems     String?             // JSON array of AI-detected items
  parseStatus       String?             // PENDING, COMPLETED, FAILED
  parseError        String?

  uploadedAt        DateTime            @default(now())
  processedAt       DateTime?

  @@index([estimationId])
  @@index([documentType])
}

// ============================================
// BUDGET ESTIMATION ENUMS
// ============================================

enum EstimationStatus {
  DRAFT
  UPLOADED
  PARSING
  REVIEWED
  SPECIFIED
  COMPLETED
  ARCHIVED
}

enum ItemType {
  DOOR
  HINGE
  LOCK
  DEADBOLT
  HANDLE
  LEVER
  KNOB
  CLOSER
  PANIC_BAR
  THRESHOLD
  WEATHERSTRIP
  PEEPHOLE
  KICKPLATE
  DOOR_STOP
  ASTRAGAL
  SWEEP
  OTHER
}

enum DoorType {
  INTERIOR_SINGLE
  INTERIOR_DOUBLE
