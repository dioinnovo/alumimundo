// This is your Prisma schema file for Alumimundo AI Platform
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// ============================================
// CORE DOMAIN MODELS
// ============================================

// Projects table - Construction projects management
model Project {
  id                String              @id @default(cuid())
  projectNumber     String              @unique
  name              String
  type              ProjectType         @default(RESIDENTIAL)
  status            ProjectStatus       @default(PLANNING)
  priority          Priority            @default(MEDIUM)
  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt
  startDate         DateTime?
  completionDate    DateTime?

  // Project Information
  location          String
  address           String?
  city              String?
  province          String?             // San José, Guanacaste, etc.
  country           String              @default("Costa Rica")

  // Client Information
  clientName        String
  clientEmail       String
  clientPhone       String
  clientCompany     String?
  architectName     String?
  contractorName    String?

  // Financial Information
  budgetAmount      Float?
  actualCost        Float?
  currency          String              @default("CRC")

  // Project Metadata
  description       String?
  notes             String?
  squareMeters      Float?

  // AI Analysis
  complexityScore   Float?
  riskScore         Float?

  // Relationships
  specifications    Specification[]
  documents         Document[]
  qualityChecks     QualityCheck[]
  activities        Activity[]
  inventoryItems    InventoryAllocation[]

  @@index([status])
  @@index([priority])
  @@index([type])
  @@index([createdAt])
}

// Specifications table - Product specifications per project
model Specification {
  id                String              @id @default(cuid())
  specNumber        String              @unique
  status            SpecificationStatus @default(DRAFT)
  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt
  approvedAt        DateTime?

  // Specification Details
  category          String              // Grifería, Cerraduras, Lavamanos, etc.
  productFamily     String?             // KOHLER, Schlage, Kallista, etc.
  quantity          Int
  unitPrice         Float?
  totalPrice        Float?

  // Product Information
  productSKU        String?
  productName       String
  productModel      String?
  finish            String?             // Chrome, Brushed Nickel, Matte Black, etc.
  specifications    String?             // JSON string with detailed specs

  // Installation Details
  installationNotes String?
  installationDate  DateTime?
  installerName     String?

  // AI Recommendations
  aiConfidence      Float?
  alternatives      String?             // JSON array of alternative products

  // Relationships
  projectId         String
  project           Project             @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
  @@index([status])
  @@index([category])
}

// Products table - Complete product catalog (ENHANCED)
model Product {
  id                String              @id @default(cuid())
  sku               String              @unique
  name              String
  nameEs            String?             // Spanish name
  brand             String              // KOHLER, Schlage, Kallista, Steelcraft
  category          String              // Grifería, Cerraduras, etc.
  subcategory       String?

  // Product Details
  description       String?
  descriptionEs     String?
  specifications    String?             // JSON string
  finish            String?
  color             String?
  material          String?

  // Compliance & Certifications
  isADACompliant    Boolean             @default(false)
  isMarineGrade     Boolean             @default(false)
  isWaterSense      Boolean             @default(false)
  certifications    String?             // Comma-separated certification codes

  // Environmental
  isEcoFriendly     Boolean             @default(false)
  sustainabilityRating Float?

  // Pricing
  costPrice         Float?
  listPrice         Float?
  currency          String              @default("CRC")
  priceCRC          Float?
  priceUSD          Float?

  // Inventory
  currentStock      Int                 @default(0)
  minStock          Int                 @default(0)
  maxStock          Int?
  reorderPoint      Int?
  leadTimeDays      Int                 @default(30)

  // Product Metadata
  weight            Float?              // in kg
  dimensions        String?             // JSON: {length, width, height} in cm
  imageUrl          String?             // Primary product image from GCS
  dataSheetUrl      String?
  installGuideUrl   String?
  warrantyYears     Int?

  // AI & Search
  embedding         String?             // Vector embedding for similarity search
  tags              String?             // Comma-separated tags
  searchKeywords    String?             // Keywords for search

  // Scraping Metadata
  sourceUrl         String?             // Original product page URL
  lastScrapedAt     DateTime?
  scrapingEnabled   Boolean             @default(true)

  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt

  // Relationships
  providerId        String?
  provider          Provider?           @relation(fields: [providerId], references: [id])
  images            ProductImage[]
  inventoryMovements InventoryMovement[]
  allocations       InventoryAllocation[]

  @@index([sku])
  @@index([brand])
  @@index([category])
  @@index([name])
  @@index([providerId])
  @@index([isADACompliant])
  @@index([isMarineGrade])
}

// Quality Checks table - Installation validation
model QualityCheck {
  id                String              @id @default(cuid())
  checkNumber       String              @unique
  type              QualityCheckType    @default(PRE_INSTALLATION)
  status            QualityStatus       @default(PENDING)
  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt
  completedAt       DateTime?

  // Check Details
  area              String              // Bathroom 1, Kitchen, Main Entrance, etc.
  productCategory   String
  inspectorName     String?

  // Quality Scores
  overallScore      Float?
  alignmentScore    Float?
  finishScore       Float?
  functionalityScore Float?

  // Visual Inspection
  photoUrls         String?             // JSON array of photo URLs
  defectsFound      String?             // JSON array of defects
  aiAnalysis        String?             // JSON with AI analysis results

  // Approval
  approved          Boolean             @default(false)
  approvedBy        String?
  rejectionReason   String?

  // Relationships
  projectId         String
  project           Project             @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
  @@index([status])
  @@index([type])
}

// Documents table - Generated and uploaded documents
model Document {
  id                String              @id @default(cuid())
  filename          String
  originalName      String
  mimeType          String
  size              Int
  url               String
  uploadedAt        DateTime            @default(now())

  // Document Type & Processing
  documentType      DocumentType        @default(OTHER)
  ocrText           String?
  metadata          String?             // JSON string
  processedAt       DateTime?

  // AI Generation
  generatedByAI     Boolean             @default(false)
  generationPrompt  String?

  // Relationships
  projectId         String
  project           Project             @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
  @@index([documentType])
}

// Inventory Movements table - Track stock changes
model InventoryMovement {
  id                String              @id @default(cuid())
  movementNumber    String              @unique
  type              MovementType
  quantity          Int

  // Movement Details
  reason            String?
  notes             String?
  reference         String?             // PO number, invoice number, etc.

  // Cost tracking
  unitCost          Float?
  totalCost         Float?

  createdAt         DateTime            @default(now())
  createdBy         String?

  // Relationships
  productId         String
  product           Product             @relation(fields: [productId], references: [id])

  @@index([productId])
  @@index([type])
  @@index([createdAt])
}

// Inventory Allocations table - Reserve stock for projects
model InventoryAllocation {
  id                String              @id @default(cuid())
  quantity          Int
  status            AllocationStatus    @default(RESERVED)

  allocatedAt       DateTime            @default(now())
  releasedAt        DateTime?

  notes             String?

  // Relationships
  projectId         String
  project           Project             @relation(fields: [projectId], references: [id], onDelete: Cascade)
  productId         String
  product           Product             @relation(fields: [productId], references: [id])

  @@index([projectId])
  @@index([productId])
  @@index([status])
}

// Providers/Manufacturers table - Track product sources
model Provider {
  id                String              @id @default(cuid())
  name              String              @unique  // KOHLER, Tarkett, Schlage
  nameEs            String?             // Spanish name if different
  website           String?
  country           String?

  // Contact Information
  contactEmail      String?
  contactPhone      String?
  salesRepName      String?

  // Scraping Metadata
  catalogUrl        String?             // Main catalog/products page URL
  lastScrapedAt     DateTime?
  scrapingEnabled   Boolean             @default(true)
  scrapingFrequency String?             // daily, weekly, monthly

  // Business Information
  accountNumber     String?
  paymentTerms      String?
  shippingMethod    String?

  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt

  // Relationships
  products          Product[]
  scrapeLogs        ScrapeLog[]

  @@index([name])
}

// Product Images table - Multiple images per product stored in GCS
model ProductImage {
  id                String              @id @default(cuid())
  productId         String
  product           Product             @relation(fields: [productId], references: [id], onDelete: Cascade)

  // Image Details
  url               String              // GCS URL: gs://alumimundo_inventory/...
  publicUrl         String?             // Public HTTPS URL
  altText           String?
  caption           String?
  displayOrder      Int                 @default(0)

  // Image Type
  imageType         ProductImageType    @default(PRODUCT_PHOTO)

  // Metadata
  width             Int?
  height            Int?
  fileSize          Int?                // in bytes
  mimeType          String?

  uploadedAt        DateTime            @default(now())

  @@index([productId])
  @@index([imageType])
}

// Scrape Logs table - Track scraping history
model ScrapeLog {
  id                String              @id @default(cuid())
  providerId        String
  provider          Provider            @relation(fields: [providerId], references: [id])

  // Scrape Details
  status            ScrapeStatus        @default(IN_PROGRESS)
  productsFound     Int                 @default(0)
  productsAdded     Int                 @default(0)
  productsUpdated   Int                 @default(0)
  imagesDownloaded  Int                 @default(0)

  // Execution Details
  startedAt         DateTime            @default(now())
  completedAt       DateTime?
  duration          Int?                // in seconds

  // Error Handling
  errors            String?             // JSON array of errors
  errorMessage      String?

  // Metadata
  scrapedBy         String?             // User or system
  notes             String?

  @@index([providerId])
  @@index([status])
  @@index([startedAt])
}

// Activity table - Audit trail for all models
model Activity {
  id                String              @id @default(cuid())
  action            String
  description       String
  userId            String?
  userEmail         String?
  metadata          String?             // JSON string
  createdAt         DateTime            @default(now())

  // Relationships
  projectId         String?
  project           Project?            @relation(fields: [projectId], references: [id], onDelete: Cascade)

  @@index([projectId])
  @@index([createdAt])
  @@index([userId])
}

// Users table - User management
model User {
  id                String              @id @default(cuid())
  email             String              @unique
  name              String
  role              UserRole            @default(USER)

  // Authentication
  passwordHash      String?
  emailVerified     Boolean             @default(false)

  // Profile
  phone             String?
  company           String?
  position          String?

  // Metadata
  createdAt         DateTime            @default(now())
  updatedAt         DateTime            @updatedAt
  lastLoginAt       DateTime?

  // Relationships
  designProjects    DesignProject[]

  @@index([email])
  @@index([role])
}

